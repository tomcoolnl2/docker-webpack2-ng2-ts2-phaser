{
  "name": "ng-game",
  "version": "1.0.0",
  "description": "Angular2 Setup for Game",
  "license": "MIT",
  "main": "",
  "engines": {
    "node": ">= 4.4.7",
    "npm": ">= 3"
  },
  "scripts": {
    "initialize": "docker-compose build",
    "clean": "rimraf dist/public && rimraf compiled",
    "tslint": "tslint ./src/app/**/*.ts",
    "build": "npm run clean && npm run aot && webpack --config webpack/webpack.prod.js  --progress --profile --env.aot",
    "serve": "webpack-dev-server --config webpack/webpack.dev.js --inline --progress --profile --watch --content-base ./src --host=0.0.0.0",
    "serve:dist": "http-server dist/public  -p 8080 ",
    "docker:install": "docker-compose run frontend npm install",
    "docker:serve:dist": "docker-compose run --service-ports frontend npm run serve-dist",
    "aot": "./node_modules/.bin/ngc -p tsconfig.aot.json"
  },
